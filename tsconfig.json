{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"]
    },
    "types": ["@astrojs/image/client"],
    "jsx": "react-jsx",
    "importsNotUsedAsValues": "error",
    "jsxImportSource": "react"
  }
}
